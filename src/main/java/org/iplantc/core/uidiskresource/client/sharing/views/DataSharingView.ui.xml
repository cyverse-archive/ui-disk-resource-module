<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container"
	xmlns:gxt="urn:import:com.sencha.gxt.widget.core.client" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:grid="urn:import:com.sencha.gxt.widget.core.client.grid"
	xmlns:button="urn:import:com.sencha.gxt.widget.core.client.button"
	xmlns:toolbar="urn:import:com.sencha.gxt.widget.core.client.toolbar"
	xmlns:form="urn:import:com.sencha.gxt.widget.core.client.form">

	<ui:with field="collaboratorsListStore" type="com.sencha.gxt.data.shared.ListStore" />
	<ui:with field="collaboratorsColumnModel"
		type="com.sencha.gxt.widget.core.client.grid.ColumnModel" />
	<ui:with field="collaboratorsGridView"
		type="com.sencha.gxt.widget.core.client.grid.GridView">
		<ui:attributes forceFit="true" autoFill="true" />
	</ui:with>
	
	<ui:with type="org.iplantc.core.uidiskresource.client.DiskResourceDisplayStrings" field="strings" />
	
	<ui:with field="diskResourcesListStore" type="com.sencha.gxt.data.shared.ListStore" />
	<ui:with field="diskResourcesColumnModel"
		type="com.sencha.gxt.widget.core.client.grid.ColumnModel" />
	<ui:with field="diskResourcesGridView"
		type="com.sencha.gxt.widget.core.client.grid.GridView">
		<ui:attributes forceFit="true" autoFill="true" />
	</ui:with>

	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="outerMargins">
		<ui:attributes top="20" right="20" bottom="20" left="20" />
	</ui:with>
	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="westMargins">
		<ui:attributes top="0" right="5" bottom="0" left="5" />
	</ui:with>
	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="centerMargins">
		<ui:attributes top="0" right="0" bottom="0" left="0" />
	</ui:with>
	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="eastMargins">
		<ui:attributes top="0" right="5" bottom="0" left="5" />
	</ui:with>

	<ui:with type="com.sencha.gxt.widget.core.client.container.MarginData"
		field="outerData">
		<ui:attributes margins="{outerMargins}" />
	</ui:with>
	<ui:with
		type="com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData"
		field="westData">
		<ui:attributes size="200" margins="{westMargins}"
			collapsible="true" split="true" />
	</ui:with>
	<ui:with type="com.sencha.gxt.widget.core.client.container.MarginData"
		field="centerData">
		<ui:attributes margins="{centerMargins}" />
	</ui:with>
	<ui:with
		type="com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData"
		field="eastData">
		<ui:attributes size="320" margins="{eastMargins}" collapsible="true" split="true" />
	</ui:with>

	<container:SimpleContainer>
		<container:child>
			<container:BorderLayoutContainer
				ui:field="con" borders="true">
				<container:west layoutData="{westData}">
					<gxt:FramedPanel ui:field="collaboratorListPnl"
						collapsible="false" headerVisible="true" headingText= "1.{strings.selectCollabs}">
						<grid:Grid ui:field="collaboratorsGrid" cm="{collaboratorsColumnModel}" store="{collaboratorsListStore}"
									view="{collaboratorsGridView}" loadMask="true" columnReordering="true"
									borders="false">
								</grid:Grid>
					</gxt:FramedPanel>
				</container:west>
				<container:center layoutData="{centerData}">
					<gxt:FramedPanel ui:field="diskResourceListPnl"
						collapsible="false" headerVisible="true" headingText="2.{strings.selectFilesFolders}">
						<grid:Grid ui:field="diskResourcesGrid" cm="{diskResourcesColumnModel}" store="{diskResourcesListStore}"
									view="{diskResourcesGridView}" loadMask="true" columnReordering="true"
									borders="false">
								</grid:Grid>
					</gxt:FramedPanel>
				</container:center>
				<container:east layoutData="{eastData}">
				<gxt:FramedPanel ui:field="permissionsPnl"
						collapsible="false" headerVisible="true" headingText="3.{strings.changePermissions}">
						</gxt:FramedPanel>
				</container:east>
			</container:BorderLayoutContainer>
		</container:child>
	</container:SimpleContainer>
</ui:UiBinder> 