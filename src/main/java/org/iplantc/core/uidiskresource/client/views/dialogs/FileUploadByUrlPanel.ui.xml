<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
    xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:form="urn:import:com.sencha.gxt.widget.core.client.form"
    xmlns:con="urn:import:com.sencha.gxt.widget.core.client.container"
    xmlns:gxt="urn:import:com.sencha.gxt.widget.core.client">

    <ui:with
        type="org.iplantc.de.resources.client.messages.IplantDisplayStrings"
        field="display" />

    <ui:with
        type="com.sencha.gxt.core.client.util.Margins"
        field="sideMargins">
        <ui:attributes
            top="4"
            right="4"
            bottom="4"
            left="4" />
    </ui:with>

    <ui:with
        type="com.sencha.gxt.core.client.util.Margins"
        field="centerMargins">
        <ui:attributes
            top="4"
            right="0"
            bottom="4"
            left="0" />
    </ui:with>

    <ui:with
        type="com.sencha.gxt.widget.core.client.container.HorizontalLayoutContainer.HorizontalLayoutData"
        field="centerLayoutData">
        <ui:attributes
            width="1"
            height="1"
            margins="{centerMargins}" />
    </ui:with>

    <ui:with
        type="com.sencha.gxt.widget.core.client.container.HorizontalLayoutContainer.HorizontalLayoutData"
        field="sideLayoutData">
        <ui:attributes
            width="-1"
            height="1"
            margins="{sideMargins}" />
    </ui:with>

    <!-- <form:FormPanel> -->
    <con:VerticalLayoutContainer>
        <g:HTML ui:field="htmlDestText" />
        <g:HTML HTML="{display.urlPrompt}" />
        <con:SimpleContainer>
            <con:HorizontalLayoutContainer>
                <con:child layoutData="{centerLayoutData}">
                    <form:TextArea debugId="idUrlImport0" ui:field="url0" />
                </con:child>
                <con:child layoutData="{sideLayoutData}">
                    <gxt:Status ui:field="formStatus0" />
                </con:child>
            </con:HorizontalLayoutContainer>
        </con:SimpleContainer>
        <con:SimpleContainer>
            <con:HorizontalLayoutContainer>
                <con:child layoutData="{centerLayoutData}">
                    <form:TextArea debugId="idUrlImport1" ui:field="url1" />
                </con:child>
                <con:child layoutData="{sideLayoutData}">
                    <gxt:Status ui:field="formStatus1" />
                </con:child>
            </con:HorizontalLayoutContainer>
        </con:SimpleContainer>
        <con:SimpleContainer>
            <con:HorizontalLayoutContainer>
                <con:child layoutData="{centerLayoutData}">
                    <form:TextArea debugId="idUrlImport2" ui:field="url2" />
                </con:child>
                <con:child layoutData="{sideLayoutData}">
                    <gxt:Status ui:field="formStatus2" />
                </con:child>
            </con:HorizontalLayoutContainer>
        </con:SimpleContainer>
        <con:SimpleContainer>
            <con:HorizontalLayoutContainer>
                <con:child layoutData="{centerLayoutData}">
                    <form:TextArea debugId="idUrlImport3" ui:field="url3" />
                </con:child>
                <con:child layoutData="{sideLayoutData}">
                    <gxt:Status ui:field="formStatus3" />
                </con:child>
            </con:HorizontalLayoutContainer>
        </con:SimpleContainer>
        <con:SimpleContainer>
            <con:HorizontalLayoutContainer>
                <con:child layoutData="{centerLayoutData}">
                    <form:TextArea debugId="idUrlImport4" ui:field="url4" />
                </con:child>
                <con:child layoutData="{sideLayoutData}">
                    <gxt:Status ui:field="formStatus4" />
                </con:child>
            </con:HorizontalLayoutContainer>
        </con:SimpleContainer>
    </con:VerticalLayoutContainer>
    <!-- </form:FormPanel> -->





</ui:UiBinder>
